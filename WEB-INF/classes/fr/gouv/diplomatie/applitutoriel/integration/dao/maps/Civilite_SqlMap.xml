<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="civilite">

	<resultMap class="fr.gouv.diplomatie.applitutoriel.business.bo.Civilite" id="mapCivilite">
		<result column="ID_CIVILITE" property="id" />
		<result column="CIV_LIBELLE" property="libelle" />
	</resultMap>

    <select id="select" resultMap="mapCivilite">
        SELECT ID_CIVILITE, CIV_LIBELLE
        FROM CIVILITE ;
    </select>

    <select id="selectByLibelle" resultMap="mapCivilite">
        SELECT ID_CIVILITE, CIV_LIBELLE
        FROM CIVILITE
        WHERE CIV_LIBELLE = #libelle#;
    </select>
</sqlMap>